        <arguments>
          <argument>
            <kind>1</kind>
            <string>///User input
//Left steering behavior
if keyboard_check(vk_left){
    if(speed != 0){
        image_index = 1;
    }
    hspeed -=0.1; //Doesn't reduce vspeed as direction +=2; hange direction
}

//Right steering behavior
if keyboard_check(vk_right){
    if(speed != 0){
        image_index = 2;
    }
    hspeed += 0.1; //direction -=2;
}

//Forward steering behavior
if keyboard_check(vk_up){
    motion_add(90, 0.1);
    if(vspeed &lt; max_speed) vspeed = max_speed;
}

//Backward steering behavior
if keyboard_check(vk_down){
    motion_add(90, -0.1);
    if(vspeed &gt; max_b_speed) vspeed = max_b_speed;
}

//imgage_index = image_number * direction /360
while(vspeed &gt; -1){//add default speed
    //vspeed += -0.001; //To be used with test field 
    break;
}
if(friction &gt; 0.06){
    friction -= 0.01;
}
if(vspeed &lt; max_speed){
    vspeed = max_speed;
}

//Check if player do not sterr eathier to the right or left, chenge car sprite to face to the front
if keyboard_check_released(vk_left) image_index = 0;
if keyboard_check_released(vk_right) image_index = 0;


</string>
          </argument>
        </arguments>
      </action>
